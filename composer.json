{
    "name": "tobscure/json-api-server",
    "require": {
        "php": "^7.2",
        "doctrine/inflector": "^1.3",
        "json-api-php/json-api": "^2.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "zendframework/zend-diactoros": "^2.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Toby Zerner",
